  description "Front"
  position 0 0 6
}
Viewpoint {
  description "Top-down"
  orientation 1 0 0 -0.6
  position 0 2 0
}
Transform {
    #rotate the shape towards the default VP
    rotation 0 0 0 0
    children
        Shape {
            appearance Appearance {
                material Material {
                #diffuseColor 0.4 0.6 0.3
                diffuseColor 0.1 0.1 1.0
                }
            }
            geometry DEF EGrid ElevationGrid {
                height [		]
                solid TRUE
                xDimension 400
                xSpacing 1
                zDimension 400
                zSpacing 1
            }
        }
}
