#X3D V3.0 utf8
PROFILE Immersive
META "filename" "ifs.x3dv"

Shape {
  geometry DEF IFS IndexedFaceSet {
    coordIndex [ 0, 1, 3, -1, 0, 2, 3, -1 ]
    coord Coordinate {
      point [ 0 0 0, 1 0 0, 1 0 -1, 0.5 1 0 ]
    }
    #if color field != null, it MUST contain at least as many colors
    #as the IFS does faces
    color Color {}
    colorPerVertex FALSE
    solid FALSE
  }
  appearance Appearance {
    material Material {
        diffuseColor 1 1 1
    }
  }
}
